{
  "name": "poc-4-doshii",
  "version": "1.0.0",
  "description": "POC for Doshii using Cypress",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "test": "cypress run",
    "runtests": "npm run test --",
    "cy:run:chrome": "$(npm bin)/cypress run --browser chrome",
    "cy:run-with-report": "cypress run && node report.js",
    "cy:tags": "./node_modules/.bin/cypress-tags run"
  },
  "keywords": [
    "Cypress"
  ],
  "author": "Cerosh",
  "license": "ISC",
  "dependencies": {
    "cypress": "^8.7.0",
    "prettier": "^2.4.1"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cucumber_report",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "devDependencies": {
    "@cucumber/cucumber": "^8.0.0-rc.1",
    "@cypress/webpack-preprocessor": "^5.9.1",
    "cucumber-html-reporter": "^5.5.0",
    "cypress-cucumber-preprocessor": "^4.3.0",
    "multiple-cucumber-html-reporter": "^1.18.0",
    "typescript": "^4.4.4"
  }
}
